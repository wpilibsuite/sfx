<fx:root type="javafx.scene.layout.VBox" alignment="TOP_CENTER" prefHeight="-1.0" prefWidth="-1.0" style="" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <children>
    <Label alignment="CENTER" contentDisplay="CENTER" text="Data Properties" textAlignment="CENTER" VBox.vgrow="ALWAYS">
      <font>
        <Font size="18.0" fx:id="x1" />
      </font>
    </Label>
    <GridPane alignment="BASELINE_LEFT" hgap="7.0" vgap="4.0">
      <children>
        <Label text="Name:" GridPane.columnIndex="0" GridPane.rowIndex="0" />

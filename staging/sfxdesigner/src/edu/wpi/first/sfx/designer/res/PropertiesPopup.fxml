<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import edu.wpi.first.sfx.designer.ui.*?>

<fx:root type="javafx.scene.layout.BorderPane" minWidth="300" style="-fx-background-color: linear-gradient(to top, #bbb 85%, #eee);&#10;-fx-background-radius: 5;&#10;-fx-border-radius: 5;" xmlns:fx="http://javafx.com/fxml">
  <center>
    <TitledFormPane prefHeight="257.0" minWidth="297" fx:id="prop_list" hgap="12.0" maxRightWidth="175.0">
      <padding>
        <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
      </padding>
	</TitledFormPane>
  </center>
  <stylesheets>
    <URL value="@css/general.css" />
  </stylesheets>
  <top>
    <HBox fx:id="titlebar" alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" onMouseDragged="#move" onMousePressed="#begin_move" onMouseReleased="#finish_move" prefHeight="-1.0" prefWidth="-1.0" spacing="5.0" style="">
      <children>
        <HBox prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
        <Label fx:id="title" alignment="TOP_LEFT" text="Awesome Control 9000" focusTraversable="true">
          <font>
            <Font name="System Bold" size="13.0" />
          </font>
        </Label>
        <HBox prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
        <Button mnemonicParsing="false" onAction="#close" style="-fx-background-color: transparent;" text="X" textFill="#df0040">
          <font>
            <Font name="System Bold" size="15.0" />
          </font>
        </Button>
      </children>
      <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
      </padding>
    </HBox>
  </top>
</fx:root>
